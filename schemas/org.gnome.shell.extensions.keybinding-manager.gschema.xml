<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.keybinding-manager"
    path="/org/gnome/shell/extensions/keybinding-manager/">
    
    <key type="as" name="assign-app-automove">
      <default><![CDATA[['<Super><Shift>A']]]></default>
      <summary>Assign app to workspace</summary>
      <description>
        Assign the focused app to its current workspace
      </description>
    </key>
    
    <key type="as" name="remove-app-automove">
      <default><![CDATA[['<Super><Shift>U']]]></default>
      <summary>Remove app to workspace</summary>
      <description>
        Remove the focused app of auto move apps
      </description>
    </key>
    
  </schema>
</schemalist>
